<templateSet group="Go Struct Tags">
    <template name="column tag" value="`column:&quot;$FIELD_NAME$&quot;$END$`"
              description="Create GoBatis Column Field Mapping"
              toReformat="false"
              toShortenFQNames="false">
        <variable name="FIELD_NAME" expression="snakeCase(fieldName())" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="GoBatis" value="true"/>
        </context>
    </template>

    <template name="column" value="column:&quot;$FIELD_NAME$&quot;$END$"
              description="Create GoBatis Column Field Mapping"
              toReformat="false"
              toShortenFQNames="false">
        <variable name="FIELD_NAME" expression="snakeCase(fieldName())" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="GoBatisTag" value="true"/>
        </context>
    </template>

</templateSet>